---
import { getRoutes } from '../helpers/routes'; // Adjust the path if needed

const routes = getRoutes();
---

<header class="bg-primary">
  <nav
    class="navbar navbar-expand-lg bg-primary container col-xxl-8"
    aria-label=""
    data-bs-theme="dark"
  >
    <div class="container-fluid">
      <button
        class="navbar-toggler"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarsExample11"
        aria-controls="navbarsExample11"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <div
        class="collapse navbar-collapse d-lg-flex justify-content-between"
        id="navbarsExample11"
      >
        <a class="navbar-brand" href="#"
          ><img
            class="mt-2 mt-lg-0"
            src="/images/umdcolorstack-icon.png"
            alt=""
            height="72"
          /></a
        >
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link active text-white" aria-current="page" href="#"
              >Home</a
            >
          </li>
          <li class="nav-item">
            <a class="nav-link text-white" href="#">Event</a>
          </li>
          <li class="nav-item">
            <a class="nav-link text-white" href="#">Contact Us</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>
</header>
